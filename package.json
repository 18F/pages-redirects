{
  "name": "pages-redirects",
  "version": "1.0.0",
  "main": "build-config.js",
  "license": "CC0-1.0",
  "scripts": {
    "eslint": "eslint",
    "test": "tape test/unit/**/*.js | tap-summary",
    "test-docker": "tape test/integration/**/*.js | tap-summary",
    "test-prod": "TARGET_HOST=https://pages.18f.gov tape test/integration/**/*.js | tap-summary",
    "clean": "rm out/*.conf",
    "build-configs": "node build-nginx-configs.js",
    "build-docker": "npm run build-configs && docker-compose build"
  },
  "dependencies": {
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "2.2.0",
    "js-yaml": "^3.8.2",
    "nunjucks": "^3.0.0",
    "request": "^2.81.0",
    "tap-summary": "^3.0.1",
    "tape": "^4.6.3"
  }
}
